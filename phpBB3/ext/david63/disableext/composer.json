{
    "name": "david63/disableext",
    "type": "phpbb-extension",
    "description": "Adds an option to disable all extensions.",
    "version": "2.1.0",
    "time": "2017-05-25",
    "keywords": [
        "phpbb",
        "extension",
        "disable"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "David Wood",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "phpbb/phpbb": ">=3.2.0",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Disable all extensions",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/disable_all_extensions",
            "filename": "version_check"
        }
    }
}
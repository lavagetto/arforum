    "homepage": "http://phpbbguru.net",
    "version": "1.0.1",
    "time": "2014-05-16",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Татьяна5",
            "homepage": "http://phpbbguru.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Profile side switcher",
        "soft-require": {
		    "phpbb/phpbb": "3.1.*@dev"
        }
    }
}
